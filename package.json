{
  "name": "vite-plugin-public-transform",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "description": "Improving the handling of static resources in Vite through replacing target string directly.",
  "author": "s3xysteak",
  "license": "MIT",
  "homepage": "https://github.com/vite-plugin-public-transform",
  "repository": {
    "type": "git",
    "url": "https://github.com/vite-plugin-public-transform.git"
  },
  "bugs": "https://github.com/vite-plugin-public-transform/issues",
  "keywords": [
    "vite",
    "assets",
    "public"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "unbuild && cd ./example && pnpm i && pnpm dev",
    "dev:preview": "unbuild && cd ./example && pnpm i && pnpm build && pnpm preview",
    "build": "tsc && unbuild",
    "build-only": "unbuild",
    "test": "vitest",
    "test-once": "vitest run",
    "release": "esno ./scripts/release.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.12.2",
    "@types/node": "^20.12.5",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "typescript": "^5.4.4",
    "unbuild": "^2.0.0",
    "vite": "^5.2.8",
    "vitest": "^1.4.0"
  }
}
